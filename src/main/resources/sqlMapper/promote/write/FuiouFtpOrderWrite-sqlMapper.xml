<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gqhmt.fss.architect.fuiouFtp.mapper.write.FuiouFtpOrderWriteMapper">

	<insert id="saveOrUpdateAll">
		insert into t_fuiou_ftp_order (order_no,file_size,file_status,upload_status,download_status,result_status,result,type,input_date,return_result_status) values
		<foreach collection="fuiouCollection"   separator=","  item="fuiouCollection" index="index">
			(${fuiouCollection.orderNo},${fuiouCollection.fileSize},${fuiouCollection.fileStatus},${fuiouCollection.uploadStatus},${fuiouCollection.downloadStatus},${fuiouCollection.resultStatus},${fuiouCollection.result},${fuiouCollection.type},${fuiouCollection.inputDate},${fuiouCollection.retrunResultStatus})
		</foreach>
	</insert>

</mapper>
